{
  "@platforms": [
    "android",
    "iPhone",
    "iPad"
  ],
  "id": "__UNI__XXXXXXX",
  "name": "mobile-app",
  "version": {
    "name": "1.0.0",
    "code": 100
  },
  "description": "uni-app 项目",
  "developer": {
    "name": "",
    "email": "",
    "url": ""
  },
  "permissions": {
    "UniNView": {
      "description": "UniNView原生渲染"
    }
  },
  "plus": {
    "useragent": {
      "value": "uni-app",
      "concatenate": true
    },
    "splashscreen": {
      "target": "id:1",
      "autoclose": true,
      "waiting": true,
      "delay": 0
    },
    "popGesture": "close",
    "launchwebview": {
      "render": "always",
      "id": "1",
      "kernel": "WKWebview"
    },
    "usingComponents": true,
    "compilerVersion": 3,
    "statusBar": {
      "style": "dark"
    },
    "statusbar": {
      "immersed": "supportedDevice",
      "style": "dark",
      "background": "#000000"
    },
    "allowsInlineMediaPlayback": true,
    "distribute": {
      "plugins": {
        "audio": {
          "mp3": {
            "description": "Android平台录音支持MP3格式文件"
          }
        }
      }
    },
    "uni-app": {
      "control": "uni-v3",
      "vueVersion": "3",
      "compilerVersion": "4.87",
      "nvueCompiler": "uni-app",
      "renderer": "auto",
      "nvue": {
        "flex-direction": "column"
      },
      "nvueLaunchMode": "normal",
      "webView": {
        "minUserAgentVersion": "49.0"
      }
    }
  },
  "app-harmony": {
    "useragent": {
      "value": "uni-app",
      "concatenate": true
    }
  },
  "launch_path": "__uniappview.html"
}