{
  "@platforms": [
    "android",
    "iPhone",
    "iPad"
  ],
  "id": "__UNI__XXXXXXX",
  "name": "mobile-app",
  "version": {
    "name": "1.0.0",
    "code": 100
  },
  "description": "uni-app 项目",
  "developer": {
    "name": "",
    "email": "",
    "url": ""
  },
  "permissions": {
    "UniNView": {
      "description": "UniNView原生渲染"
    }
  },
  "plus": {
    "useragent": {
      "value": "uni-app",
      "concatenate": true
    },
    "splashscreen": {
      "target": "id:1",
      "autoclose": true,
      "waiting": true,
      "delay": 0
    },
    "popGesture": "close",
    "launchwebview": {
      "id": "1",
      "kernel": "WKWebview"
    },
    "usingComponents": true,
    "compilerVersion": 3,
    "statusBar": {
      "style": "dark"
    },
    "statusbar": {
      "immersed": "supportedDevice",
      "style": "light",
      "background": "#1296db"
    },
    "allowsInlineMediaPlayback": true,
    "distribute": {
      "plugins": {
        "audio": {
          "mp3": {
            "description": "Android平台录音支持MP3格式文件"
          }
        }
      }
    },
    "safearea": {
      "background": "#ffffff",
      "bottom": {
        "offset": "auto"
      }
    },
    "uni-app": {
      "control": "uni-v3",
      "vueVersion": "3",
      "compilerVersion": "4.87",
      "nvueCompiler": "uni-app",
      "renderer": "auto",
      "nvue": {
        "flex-direction": "column"
      },
      "nvueLaunchMode": "normal",
      "webView": {
        "minUserAgentVersion": "49.0"
      }
    },
    "tabBar": {
      "position": "bottom",
      "color": "#8a8a8a",
      "selectedColor": "#1296db",
      "borderStyle": "rgba(0,0,0,0.4)",
      "blurEffect": "none",
      "fontSize": "10px",
      "iconWidth": "24px",
      "spacing": "3px",
      "height": "50px",
      "list": [
        {
          "pagePath": "src/pages/index/index",
          "text": "首页",
          "iconPath": "/static/tabbar/home.png",
          "selectedIconPath": "/static/tabbar/home.png"
        },
        {
          "pagePath": "src/pages/project/project",
          "text": "项目",
          "iconPath": "/static/tabbar/project.png",
          "selectedIconPath": "/static/tabbar/project.png"
        },
        {
          "pagePath": "src/pages/contact/contact",
          "text": "消息",
          "iconPath": "/static/tabbar/message.png",
          "selectedIconPath": "/static/tabbar/message.png"
        },
        {
          "pagePath": "src/pages/profile/profile",
          "text": "我的",
          "iconPath": "/static/tabbar/profile.png",
          "selectedIconPath": "/static/tabbar/profile.png"
        }
      ],
      "backgroundColor": "#ffffff",
      "selectedIndex": 0,
      "shown": true,
      "child": [
        "lauchwebview"
      ],
      "selected": 0
    }
  },
  "app-harmony": {
    "useragent": {
      "value": "uni-app",
      "concatenate": true
    },
    "safearea": {
      "background": "#ffffff",
      "bottom": {
        "offset": "auto"
      }
    }
  },
  "launch_path": "__uniappview.html"
}